// Switch case comparison, evaluating lambda object matching expression.
.source:foo
.result
switch:x:@.source
   case:foo

      // Lambda object to evaluate if expression's value equals 'foo'.
      set-value:x:@.result
         .:Is foo
   case:bar

      // Lambda object to evaluate if expression's value equals 'bar'.
      set-value:x:@.result
         .:Is bar
   default

      /*
       * Lambda object to evaluate if expression is not matched to any
       * of the above values.
       */
      set-value:x:@.result
         .:None of the above
